#include<iostream>
using namespace std;

void print(int i)
{

    cout<<"Here is the int () :"<<i<<endl;

}
void print(double i)
{
    cout<<"Here is the double () :"<<i<<endl;
}
void print(char c)
{
    cout<<"Here is the char () :"<<c<<endl;
}
int main()
{

    print(120);
    print(90.23);
    print('c');
    return 0;
}
//output
//Here is the int () :120
//Here is the double () :90.23
//Here is the char () :c
